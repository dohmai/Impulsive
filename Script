LoadedTime = tick()
--- // Impulsive Logger  // ---
----- // Aimbot Libary // -----
loadstring(game:HttpGet("https://raw.githubusercontent.com/dohmai/Impulsive/main/Libary/Aimbot"))()
----- // ESP Libary // -----
local Sense = loadstring(game:HttpGet('https://raw.githubusercontent.com/dohmai/Impulsive/main/Libary/ESP'))()
Sense.Load()
----- // Libary // -----
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/dohmai/Impulsive/main/Libary/UI"))({cheatname = "Impulsive", gamename = "Universal"})
library:init()
local menu = library.NewWindow({title = "Impulsive | Universal", size = UDim2.new(0, 482, 0.57, 6)})
----- // Tabs // -----
local AimingTab = menu:AddTab("  Combat  ")
local VisualsTab = menu:AddTab("  Visuals  ")
local PlayerTab = menu:AddTab("  Player  ")
local TeleportsTab = menu:AddTab("  Teleports  ")
local SettingsTab = library:CreateSettingsTab(menu)
----- // Sections // -----
local CTab1 = AimingTab:AddSection("Tab 1", 1)
local CTab2 = AimingTab:AddSection("Tab 2", 2)

local VTab1 = VisualsTab:AddSection("Tab 1", 1)
local VTab2 = VisualsTab:AddSection("Tab 2", 2)

local PTab1 = PlayerTab:AddSection("Tab 1", 1)
local PTab2 = PlayerTab:AddSection("Tab 2", 2)

local TTab1 = TeleportsTab:AddSection("Tab 1", 1)
local TTab2 = TeleportsTab:AddSection("Tab 2", 2)

-----

-----------------
wait()
library:SendNotification(library.cheatname.." | Loaded in "..tostring(LoadedTime), 3)
-----

function EspInterface.getWeapon(player)
  return "Unknown";
end

function EspInterface.isFriendly(player)
  return player.Team and player.Team == localPlayer.Team;
end

function EspInterface.getCharacter(player)
  return player.Character;
end

function EspInterface.getHealth(character)
  local humanoid = character and findFirstChildOfClass(character, "Humanoid");
  if humanoid then
    return humanoid.Health, humanoid.MaxHealth;
  end
  return 100, 100;
end
