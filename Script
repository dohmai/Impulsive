local speed = Walkspeed:AddToggle({text = "Enable", flag = "enableWalkspeed",
       callback = function(value)
               getgenv().CSpeed = value
       end}):AddBind({text = "Walkspeed Enabled", state = false, mode = 'toggle', bind = 'None', flag = "A2", callback = function(value) getgenv().CSpeed = value end})  
       

Walkspeed:AddSlider({text = "Speed", flag = "speedValue", suffix = "", value = 0.35, min = 0, max = 5, increment = 0.05, callback = function(value) getgenv().CSpeedValue = value end})

-- // Speed // --
game:GetService("RunService").Heartbeat:Connect(
    function()
        if getgenv().CSpeed == true then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame +
                game.Players.LocalPlayer.Character.Humanoid.MoveDirection * CSpeedValue
        end
    end)

mainSection:AddButton({text = "Unload", confirm = true,
       callback = function(bool)
           if bool then
               library:Unload() Sense.Unload() getgenv().Aimbot.Settings.Enabled = false
           else
               library:Unload() Sense.Unload() getgenv().Aimbot.Settings.Enabled = false
           end
       end})
-----------------
wait(0.15)
library:SendNotification(library.cheatname..' | Loaded ', 3)
-----

function EspInterface.getWeapon(player)
  return "Unknown";
end

function EspInterface.isFriendly(player)
  return player.Team and player.Team == localPlayer.Team;
end

function EspInterface.getCharacter(player)
  return player.Character;
end

function EspInterface.getHealth(character)
  local humanoid = character and findFirstChildOfClass(character, "Humanoid");
  if humanoid then
    return humanoid.Health, humanoid.MaxHealth;
  end
  return 100, 100;
end
