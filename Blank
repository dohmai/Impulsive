    esp.Settings.Tool.Color = bool
end})

PlayerESP:AddSeparator({
text = "Settings "
})

PlayerESP:AddToggle({
text = "Team Check",
flag = "",
callback = function(bool)
    esp.Settings.Team_Check = bool
end})

PlayerESP:AddSlider({
text = "Max Distance",
flag = "",
suffix = 'm',
value = 7500,
min = 0,
max = 7500,
increment = 25,
callback = function(bool)
       esp.Settings.Maximal_Distance = bool     
end})
----- // LOADED // -----

wait(0.2)
library:SendNotification(library.cheatname.." | Loaded in "..tostring(LoadedTime), 3)
